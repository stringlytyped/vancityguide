<h1>Pois</h1>

<table>
  <thead>
    <tr>
      <th>Public</th>
      <th>Featured</th>
      <th>Name</th>
      <th>Rating</th>
      <th>Website</th>
      <th>Price</th>
      <th>Description</th>
      <th>User</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pois.each do |poi| %>
      <tr>
        <td><%= poi.public %></td>
        <td><%= poi.featured %></td>
        <td><%= poi.name %></td>
        <td><%= poi.rating %></td>
        <td><%= poi.website %></td>
        <td><%= poi.price %></td>
        <td><%= poi.description %></td>
        <td><%= poi.owner %></td>
        <td><%= poi.category %></td>
        <td><%= link_to 'Show', poi %></td>
        <td><%= link_to 'Edit', edit_poi_path(poi) %></td>
        <td><%= link_to 'Destroy', poi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poi', new_poi_path %>
